{
  "batiq": "1",
  "info": {
    "name": "app"
  },
  "platform": "web",
  "config": {
    "link_prefixes": ["http://localhost:4400/", "app://"]
  },
  "datasource": {
    "qore": {
      "type": "qore",
      "config": {
        "endpoint": "https://batiq.qorebase.io"
      }
    },
    "qore2": {
      "type": {
        "from": "@batiq/data",
        "name": "CustomDataSource"
      },
      "config": {
        "endpoint": "https://batiq.qorebase.io"
      },
      "authenticatedRoutes": ["/users", "/posts/*"]
    },
    "petstore": {
      "type": {
        "from": "@batiq/data",
        "name": "OpenAPI"
      },
      "config": {
        "definition": "https://petstore3.swagger.io/api/v3/openapi.json",
        "auth": {
          "apiKey": null
        }
      }
    }
  },
  "components": {
    "CompoundHeader": {
      "definitions": {
        "color": {
          "type": "string",
          "default": "green"
        }
      },
      "component": {
        "from": "@batiq/components",
        "name": "Header",
        "id": "HeaderId",
        "properties": {
          "color": {
            "type": "expression",
            "expression": "props.color"
          }
        },
        "children": [
          {
            "type": "component",
            "from": "@batiq/components",
            "name": "question2",
            "id": "color",
            "properties": {
              "color": "blue",
              "color2": "red"
            },
            "children": []
          },
          {
            "type": "component",
            "from": "@batiq/components",
            "name": "question2",
            "id": "question2_2",
            "properties": {
              "color": "blue",
              "color2": "red"
            },
            "children": [
              {
                "type": "component",
                "from": "@batiq/components",
                "name": "question3",
                "id": "question3",
                "properties": {
                  "color": "blue",
                  "color2": "red"
                },
                "children": [
                  {
                    "type": "component",
                    "$ref": ""
                  }
                ]
              },
              {
                "type": "component",
                "$ref": ""
              }
            ]
          }
        ]
      }
    }
  },
  "pages": [
    {
      "name": "page 1",
      "navigation": {
        "path": "/page-1",
        "tab": {
          "label": "Home",
          "icon": "home"
        }
      },
      "children": [
        {
          "type": "component",
          "from": "@batiq/components",
          "name": "Provider",
          "properties": {},
          "children": [
            {
              "type": "component",
              "from": "local",
              "name": "CompoundHeader",
              "properties": {
                "color": "red"
              },
              "overrideProperties": {
                "question2": {
                  "color": "blue"
                },
                "question2_2": {
                  "color": "green"
                }
              },
              "overridesComponents": {
                "question2": {
                  "type": "component",
                  "from": "@batiq/components",
                  "name": "question2",
                  "properties": {
                    "color": "blue",
                    "color2": "red"
                  },
                  "children": []
                }
              }
            },
            {
              "type": "component",
              "from": "@batiq/components",
              "name": "Button",
              "properties": {
                "onPress": {
                  "type": "action",
                  "from": "@batiq/actions",
                  "name": "navigate",
                  "arguments": ["/page-4"]
                }
              },
              "children": [
                {
                  "type": "component",
                  "from": "@batiq/components",
                  "name": "Text",
                  "properties": {
                    "text": "Page 4"
                  },
                  "children": []
                }
              ]
            },
            {
              "type": "data",
              "data": "petstore",
              "name": "pet",
              "query": {
                "operationId": "getPetById",
                "parameters": {
                  "petId": 1
                }
              },
              "children": [
                {
                  "type": "component",
                  "from": "@batiq/components",
                  "name": "Text",
                  "properties": {
                    "text": {
                      "type": "expression",
                      "expression": "pet.id"
                    }
                  },
                  "children": []
                }
              ]
            },
            {
              "type": "component",
              "from": "@batiq/components",
              "name": "Text",
              "properties": {
                "text": "Page 2",
                "onPress": [
                  {
                    "type": "action",
                    "from": "@batiq/actions",
                    "name": "navigate",
                    "arguments": ["/page-2"],
                    "next": 1
                  },
                  {
                    "type": "action",
                    "from": "@batiq/actions",
                    "name": "goBack",
                    "arguments": []
                  }
                ]
              },
              "children": []
            },
            {
              "type": "component",
              "from": "@batiq/components",
              "name": "Text",
              "properties": {
                "text": "Page 2",
                "onPress": [
                  {
                    "type": "action",
                    "from": "@batiq/actions",
                    "name": "navigate",
                    "arguments": ["/page-2"],
                    "next": 1
                  },
                  {
                    "type": "action",
                    "from": "@batiq/actions",
                    "name": "goBack",
                    "arguments": []
                  }
                ]
              },
              "children": []
            },
            {
              "type": "component",
              "from": "@batiq/components",
              "name": "Link",
              "properties": {
                "text": "Page 2",
                "to": "/page-2"
              },
              "children": []
            }
          ]
        }
      ]
    },
    {
      "name": "page 2",
      "navigation": {
        "path": "/page-2"
      },
      "children": []
    },
    {
      "name": "page 3",
      "navigation": {
        "path": "/page-3",
        "tab": {
          "label": "About",
          "icon": "about"
        }
      },
      "children": []
    },
    {
      "name": "page 4",
      "navigation": {
        "path": "/page-4",
        "tab": {
          "label": "Home",
          "icon": "home"
        }
      },
      "children": []
    }
  ]
}
