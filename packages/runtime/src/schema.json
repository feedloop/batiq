{
  "batiq": "1",
  "info": {
    "name": "app"
  },
  "platform": "web",
  "config": {
    "link_prefixes": ["http://localhost:4400/", "app://"]
  },
  "theme": {
    "dark": false,
    "colors": {
      "primary": "rgb(0, 122, 255)",
      "background": "rgb(242, 242, 242)",
      "card": "rgb(255, 255, 255)",
      "text": "rgb(28, 28, 30)",
      "border": "rgb(216, 216, 216)",
      "notification": "rgb(255, 59, 48)"
    },
    "breakpoints": {
      "base": 0,
      "sm": 0,
      "md": 480,
      "lg": 768,
      "xl": 992,
      "2xl": 1280
    }
  },
  "pages": [
    {
      "name": "page 1",
      "navigation": {
        "path": "/page-1",
        "tab": {
          "label": "Home",
          "icon": "home"
        }
      },
      "children": [
        {
          "type": "component",
          "from": "@batiq/components",
          "name": "Provider",
          "properties": {},
          "children": [
            {
              "type": "component",
              "from": "@batiq/components",
              "name": "Button",
              "properties": {
                "onPress": {
                  "type": "action",
                  "from": "@batiq/actions",
                  "name": "navigate",
                  "arguments": ["/page-4"]
                }
              },
              "children": [
                {
                  "type": "component",
                  "from": "@batiq/components",
                  "name": "Text",
                  "properties": {
                    "text": "Page 4"
                  },
                  "children": []
                }
              ]
            },
            {
              "type": "component",
              "from": "@batiq/components",
              "name": "Text",
              "properties": {
                "text": "Page 2",
                "onPress": [
                  {
                    "type": "action",
                    "from": "@batiq/actions",
                    "name": "navigate",
                    "arguments": ["/page-2"],
                    "next": 1
                  },
                  {
                    "type": "action",
                    "from": "@batiq/actions",
                    "name": "goBack",
                    "arguments": []
                  }
                ]
              },
              "children": []
            },
            {
              "type": "component",
              "from": "@batiq/components",
              "name": "Link",
              "properties": {
                "text": {
                  "type": "breakpoint",
                  "breakpoints": {
                    "base": "base",
                    "sm": "sm",
                    "md": "md",
                    "xl": "xl",
                    "2xl": "2xl"
                  }
                },
                "to": "/page-2"
              },
              "children": []
            }
          ]
        }
      ]
    },
    {
      "name": "page 2",
      "navigation": {
        "path": "/page-2"
      },
      "children": []
    },
    {
      "name": "page 3",
      "navigation": {
        "path": "/page-3",
        "tab": {
          "label": "About",
          "icon": "about"
        }
      },
      "children": []
    },
    {
      "name": "page 4",
      "navigation": {
        "path": "/page-4",
        "tab": {
          "label": "Home",
          "icon": "home"
        }
      },
      "children": []
    }
  ]
}
