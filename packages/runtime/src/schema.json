{
  "batiq": "1",
  "info": {
    "name": "app"
  },
  "platform": "web",
  "config": {
    "link_prefixes": ["http://localhost:4400/", "app://"]
  },
  "datasource": {
    "qore": {
      "type": "qore",
      "config": {
        "endpoint": "https://batiq.qorebase.io"
      }
    },
    "qore2": {
      "type": {
        "from": "@batiq/data",
        "name": "CustomDataSource"
      },
      "config": {
        "endpoint": "https://batiq.qorebase.io"
      },
      "authenticatedRoutes": ["/users", "/posts/*"]
    }
  },
  "pages": [
    {
      "name": "page 1",
      "navigation": {
        "path": "/page-1",
        "tab": {
          "label": "Home",
          "icon": "home"
        }
      },
      "children": [
        {
          "type": "component",
          "from": "@batiq/components",
          "name": "Provider",
          "properties": {},
          "children": [
            {
              "type": "component",
              "from": "@batiq/components",
              "name": "Button",
              "properties": {
                "onPress": [
                  {
                    "type": "action",
                    "from": "@batiq/actions",
                    "name": "navigate",
                    "arguments": ["/page-4"],
                    "next": 1
                  },
                  {
                    "type": "action",
                    "from": "@batiq/actions",
                    "name": "goBack",
                    "arguments": []
                  }
                ]
              },
              "children": [
                {
                  "type": "component",
                  "from": "@batiq/components",
                  "name": "Text",
                  "properties": {
                    "text": "Page 4"
                  },
                  "children": []
                }
              ]
            },
            {
              "type": "data",
              "datasource": "qore2",
              "children": [
                {
                  "type": "component",
                  "from": "@batiq/data",
                  "name": "DataSource",
                  "properties": {
                    "data": "qore",
                    "name": "qore_users",
                    "query": {
                      "column": {}
                    }
                  }
                }
              ]
            },
            {
              "type": "component",
              "from": "@batiq/components",
              "name": "Text",
              "properties": {
                "text": "Page 2",
                "onPress": [
                  {
                    "type": "action",
                    "from": "@batiq/actions",
                    "name": "navigate",
                    "arguments": ["/page-2"],
                    "next": 1
                  },
                  {
                    "type": "action",
                    "from": "@batiq/actions",
                    "name": "goBack",
                    "arguments": []
                  }
                ]
              },
              "children": []
            },
            {
              "type": "component",
              "from": "@batiq/components",
              "name": "Link",
              "properties": {
                "text": "Page 2",
                "to": "/page-2"
              },
              "children": []
            }
          ]
        }
      ]
    },
    {
      "name": "page 2",
      "navigation": {
        "path": "/page-2"
      },
      "children": []
    },
    {
      "name": "page 3",
      "navigation": {
        "path": "/page-3",
        "tab": {
          "label": "About",
          "icon": "about"
        }
      },
      "children": []
    },
    {
      "name": "page 4",
      "navigation": {
        "path": "/page-4",
        "tab": {
          "label": "Home",
          "icon": "home"
        }
      },
      "children": []
    }
  ]
}
